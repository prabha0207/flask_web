from flask import Flask
app = Flask(__name__)

@app.route('/welcome')
def welcome():
   return 'welcome_my_college'
@app.route('/about')
def about():
   return 'about_us'
@app.route('/contact')
def contact():
   return 'contact_number'
@app.route('/project')
def project():
   return 'project_expo'

if __name__ == '__main__':
    app.debug = True
    app.run()


